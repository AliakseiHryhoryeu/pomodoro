{
	"name": "Pomodoro timer",
	"version": "1.0.0",
	"description": "Pomodoro timer, source code - https://github.com/AliakseiHryhoryeu/pomodoro/tree/main/chrome-extension",
	"permissions": ["storage", "fontSettings"],
	"action": {
		"default_icon": {
			"16": "./img/icon-16.png",
			"32": "./img/icon-32.png",
			"48": "./img/icon-48.png",
			"64": "./img/icon-64.png",
			"128": "./img/icon-128.png"
		},
		"default_title": "Pomodoro timer",
		"default_popup": "index.html"
	},
	"manifest_version": 3
}
